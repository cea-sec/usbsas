syntax = "proto3";
package analyzer;
import "common.proto3";

/* Requests */

message RequestAnalyze {
  string id = 1;
};

message RequestEnd {
};

message Request {
  oneof msg {
    RequestAnalyze Analyze = 1;
    RequestEnd End = 2;
  }
};


/* Response */

message ResponseAnalyze {
  string report = 1;
}

message Response {
  oneof msg {
    ResponseAnalyze Analyze = 1;
    common.ResponseEnd End = 2;
    common.ResponseError Error = 3;
    common.ResponseStatus Status = 4;
  }
};
