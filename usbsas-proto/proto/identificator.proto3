syntax = "proto3";
package identificator;
import "common.proto3";


/* Requests */

message RequestEnd {
};

message RequestId {
};

message Request {
  oneof msg {
    RequestEnd End = 1;
    RequestId Id = 2;
  }
};

/* Responses */
message ResponseId {
  string id = 1;
};

message Response {
  oneof msg {
    common.ResponseEnd End = 1;
    common.ResponseError Error = 2;
    ResponseId Id = 3;
    common.ResponseStatus Status = 4;
  }
};
