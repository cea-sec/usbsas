syntax = "proto3";
package identifier;
import "common.proto3";


/* Requests */

message RequestEnd {
};

message RequestUserId {
};

message Request {
  oneof msg {
    RequestEnd End = 1;
    RequestUserId UserId = 2;
  }
};

/* Responses */
message ResponseUserId {
  string userid = 1;
};

message Response {
  oneof msg {
    common.ResponseEnd End = 1;
    common.ResponseError Error = 2;
    ResponseUserId UserId = 3;
    common.ResponseStatus Status = 4;
  }
};
